{
  "version": 2,
  "name": "nodes",
  "scope": "nodes",
  "builds": [
    { "src": "web/package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/web/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1",
      "continue": true
    }
  ],
  "build": {
    "env": {
      "HUB_API_URL": "@hub_api_url"
    }
  }
}
